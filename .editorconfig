# editorconfig.org
root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
max_line_length = 80
ij_typescript_spaces_within_imports = true

[*.{js,jsx,ts,tsx}]
quote_type = single

[*.md]
trim_trailing_whitespace = false
